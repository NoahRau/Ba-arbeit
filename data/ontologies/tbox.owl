        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#comprises"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#Saddle"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#comprises"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#SeatPost"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>central area of the bike including the saddle and the seat post.</rdfs:comment>
        <rdfs:label xml:lang="en">SeatArea</rdfs:label>
    </owl:Class>
    


