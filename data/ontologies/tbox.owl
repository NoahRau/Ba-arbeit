                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#Frame"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">the part of frame tube that contains the seatpost of the bike, which connects to the saddle.</rdfs:comment>
        <rdfs:label xml:lang="en">SeatTube</rdfs:label>
    </owl:Class>
    


