            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">the part of a wheel to which the tire is attached and often forms part of the braking mechanism.</rdfs:comment>
        <rdfs:label xml:lang="en">Rim</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bikeo#Saddle -->

    <owl:Class rdf:about="http://purl.org/ontology/bikeo#Saddle">
        <rdfs:subClassOf rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
