<?xml version="1.0"?>
<rdf:RDF xmlns="urn:webprotege:ontology:39025fa1-ef5a-42e5-9186-5ac2214979e6"
     xml:base="urn:webprotege:ontology:39025fa1-ef5a-42e5-9186-5ac2214979e6"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:vso="http://purl.org/vso/ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:bikeo="http://purl.org/ontology/bikeo#">
    <owl:Ontology rdf:about="http://purl.org/ontology/bikeo#"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/vso/ns#modelDate -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vso/ns#modelDate"/>
    


    <!-- http://purl.org/vso/ns#weight -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vso/ns#weight"/>
    


    <!-- http://www.w3.org/2002/07/owl#sameAs -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#sameAs">
        <rdfs:label xml:lang="en">owl:sameAs</rdfs:label>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ontology/bikeo#comprises -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#comprises">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/bikeo#isComprisedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#CommonPartArea"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
        <rdfs:comment xml:lang="en">is a property defining which common part comprises a given piece.</rdfs:comment>
        <rdfs:label xml:lang="en">comprises</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#hasBrand -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#hasBrand">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/bikeo#isBrandOf"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Bike"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#Brand"/>
        <rdfs:comment xml:lang="en">is a property defining which bike in particular has a given brand.</rdfs:comment>
        <rdfs:label xml:lang="en">hasBrand</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#hasPart -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#hasPart">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/bikeo#isPartOf"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Bike"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
        <rdfs:comment xml:lang="en">is a property specifying how a bike is composed in its specific pieces.</rdfs:comment>
        <rdfs:label xml:lang="en">hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#hasProblem -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#hasProblem">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/bikeo#isProblemOf"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#Problem"/>
        <rdfs:comment xml:lang="en">is a property defining a problem that could affect a bike or a piece of a bike.</rdfs:comment>
        <rdfs:label xml:lang="en">hasProblem</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#hasSolution -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#hasSolution">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/bikeo#isSolutionOf"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Problem"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#Solution"/>
        <rdfs:comment xml:lang="en">is the property telling how a problem of a bike can be solved and through which tools.</rdfs:comment>
        <rdfs:label>hasSolution</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#hasType -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#hasType">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/bikeo#isTypeOf"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Bike"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#TypeOfBike"/>
        <rdfs:comment xml:lang="en">is a property that defines the specific type of bike of a bike model.</rdfs:comment>
        <rdfs:label xml:lang="en">hasType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#isBrandOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#isBrandOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Brand"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#Bike"/>
        <rdfs:comment xml:lang="en">is a property used for specifying which specific brand owns and produces a given bike model.</rdfs:comment>
        <rdfs:label xml:lang="en">isBrandOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#isComprisedIn -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#isComprisedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#CommonPartArea"/>
        <rdfs:comment xml:lang="en">is a property defining which piece is comprised in a given common part.</rdfs:comment>
        <rdfs:label xml:lang="en">isComprisedIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#isPartOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#isPartOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#Bike"/>
        <rdfs:comment xml:lang="en">is a property used for specifying which common parts are included in a bike.</rdfs:comment>
        <rdfs:label xml:lang="en">isPartOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#isProblemOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#isProblemOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Problem"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
        <rdfs:comment xml:lang="en">is a property defining to which piece is connected a given problem.</rdfs:comment>
        <rdfs:label xml:lang="en">isProblemOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#isRequestedFor -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#isRequestedFor">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/bikeo#requests"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Tool"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#Solution"/>
        <rdfs:comment xml:lang="en">is a property defining which tool is required in order to obtain a given solution to a problem.</rdfs:comment>
        <rdfs:label xml:lang="en">isRequestedFor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#isSolutionOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#isSolutionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Solution"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#Problem"/>
        <rdfs:comment xml:lang="en">is a property defining to which piece is connected a given solution of a problem.</rdfs:comment>
        <rdfs:label xml:lang="en">isSolutionOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#isTypeOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#isTypeOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#TypeOfBike"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#Bike"/>
        <rdfs:comment xml:lang="en">this property defines that a specific bike model is a type of bike.</rdfs:comment>
        <rdfs:label xml:lang="en">isTypeOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#producedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#producedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/bikeo#produces"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#Brand"/>
        <rdfs:comment xml:lang="en">defines the specific part of a bike that is produced by a given brand.</rdfs:comment>
        <rdfs:label xml:lang="en">producedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#produces -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#produces">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Brand"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
        <rdfs:comment xml:lang="en">is a property defining which brand produces a specific part of a bike.</rdfs:comment>
        <rdfs:label xml:lang="en">produces</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bikeo#requests -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/bikeo#requests">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Solution"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bikeo#Tool"/>
        <rdfs:comment xml:lang="en">is a property defining which solution to a problem requests a given tool to be used.</rdfs:comment>
        <rdfs:label xml:lang="en">requests</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ontology/bikeo#hasChainRing -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/bikeo#hasChainRing">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Crank"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">is a property describing the chain ring(s) attached to the crank arm. The chainring, or sprocket, is a profiled wheel with teeth that mesh with the chain.</rdfs:comment>
        <rdfs:label xml:lang="en">hasChainRing</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bikeo#hasGearing -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/bikeo#hasGearing">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Bike"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">the value describing the gearing of a bike.</rdfs:comment>
        <rdfs:label xml:lang="en">hasGearing</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bikeo#hasSize -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/bikeo#hasSize">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bikeo#Tire"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">the property for describing the width of tires.</rdfs:comment>
        <rdfs:label xml:lang="en">hasSize</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ontology/bikeo#AdjustSeatpost -->

    <owl:Class rdf:about="http://purl.org/ontology/bikeo#AdjustSeatpost">
        <rdfs:subClassOf rdf:resource="http://purl.org/ontology/bikeo#Solution"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#isSolutionOf"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#SeatHeight"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#requests"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#AllenKey"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">the possible solution to a wrong seat height that can be changed through the use of the allen key.</rdfs:comment>
        <rdfs:label xml:lang="en">AdjustSeatpost</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bikeo#AllenKey -->

    <owl:Class rdf:about="http://purl.org/ontology/bikeo#AllenKey">
        <rdfs:subClassOf rdf:resource="http://purl.org/ontology/bikeo#Tool"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#isRequestedFor"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#AdjustSeatpost"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#isRequestedFor"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#Tighten"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>a simple driver for bolts or screws that have heads with internal hexagonal recesses (sockets).</rdfs:comment>
        <rdfs:label xml:lang="en">AllenKey</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bikeo#Battery -->

    <owl:Class rdf:about="http://purl.org/ontology/bikeo#Battery">
        <rdfs:subClassOf rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
        <rdfs:comment xml:lang="en">rechargeable batteries used by E-bikes in addition to electric motors and some form of control.</rdfs:comment>
        <rdfs:label xml:lang="en">Battery</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bikeo#Bike -->

    <owl:Class rdf:about="http://purl.org/ontology/bikeo#Bike">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#hasBrand"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#Brand"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#hasType"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#TypeOfBike"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#hasGearing"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A bike or bicycle is a two-wheeled steerable machine that is pedaled by the riderâ€™s feet.</rdfs:comment>
        <rdfs:label xml:lang="en">Bike</rdfs:label>
        <owl:sameAs xml:lang="en">vso:Bicycle</owl:sameAs>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bikeo#Brakes -->

    <owl:Class rdf:about="http://purl.org/ontology/bikeo#Brakes">
        <rdfs:subClassOf rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#hasProblem"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#WornBrakes"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">devices acting on the wheel used to stop or slow down a bicycle.</rdfs:comment>
        <rdfs:label xml:lang="en">Brakes</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bikeo#Brand -->

    <owl:Class rdf:about="http://purl.org/ontology/bikeo#Brand">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#isBrandOf"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#Bike"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#produces"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">a company or organization which has produced the bike or its parts.</rdfs:comment>
        <rdfs:label xml:lang="en">Brand</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bikeo#BrokenPart -->

    <owl:Class rdf:about="http://purl.org/ontology/bikeo#BrokenPart">
        <rdfs:subClassOf rdf:resource="http://purl.org/ontology/bikeo#Problem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#hasSolution"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#ReplacePart"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#isProblemOf"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">all the parts of a bike in need of some adjustments.</rdfs:comment>
        <rdfs:label xml:lang="en">BrokenPart</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bikeo#Chain -->

    <owl:Class rdf:about="http://purl.org/ontology/bikeo#Chain">
        <rdfs:subClassOf rdf:resource="http://purl.org/ontology/bikeo#CommonPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#isComprisedIn"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#ChainArea"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">a series of (usually metal) rings or links fitted into one another to make a flexible ligament.</rdfs:comment>
        <rdfs:label xml:lang="en">Chain</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bikeo#ChainArea -->

    <owl:Class rdf:about="http://purl.org/ontology/bikeo#ChainArea">
        <rdfs:subClassOf rdf:resource="http://purl.org/ontology/bikeo#CommonPartArea"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#comprises"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#Chain"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#comprises"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#FrontDerailleur"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#hasProblem"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#SnappedChain"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>the structural part of the bike, fitted together with the frame and the wheels, that includes the chain and the front derailleur.</rdfs:comment>
        <rdfs:label xml:lang="en">ChainArea</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bikeo#ChainLink -->

    <owl:Class rdf:about="http://purl.org/ontology/bikeo#ChainLink">
        <rdfs:subClassOf rdf:resource="http://purl.org/ontology/bikeo#Tool"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#isRequestedFor"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#RefitChain"/>
            </owl:Restriction>
