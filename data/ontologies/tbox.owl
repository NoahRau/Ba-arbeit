        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#requests"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#ChainLink"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/ontology/bikeo#requests"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/ontology/bikeo#ChainRivet"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>act of reconnecting the chain which has previously broken and exited from its track.</rdfs:comment>
        <rdfs:label xml:lang="en">RefitChain</rdfs:label>
    </owl:Class>
    


